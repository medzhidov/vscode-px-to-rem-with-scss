{
    "name": "px-to-rem-with-scss",
    "displayName": "Px to rem with scss",
    "description": "Extension that make adaptive quickly with scss function.",
    "version": "1.1.2",
    "publisher": "medzhidov",
    "engines": {
        "vscode": "^1.5.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.active"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.active",
                "title": "Px to rem"
            }
        ],
        "keybindings": [
            {
                "command": "extension.active",
                "key": "alt+c",
                "mac": "alt+c",
                "when": "editorTextFocus"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^10.2.0",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/dslpp056193/vscode-px-to-rem-with-scss"
    },
    "license": "MIT"
}
